	"license": "MIT",
	"scripts": {
		"dev": "webpack serve --config ././webpack/webpack.dev.js --node-env=development",
		"build": "webpack --config ./webpack/webpack.prod.js --node-env=production && serve -s build",
		"format": "prettier --write ./src/*",
